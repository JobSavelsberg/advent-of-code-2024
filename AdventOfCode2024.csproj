<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <!-- Add all .txt files (not in bin or obj) to output-->
  <ItemGroup>
    <Content Include="**\*.txt" Exclude="bin\**;obj\**" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>
</Project>
